 <!-- Gazebo Plugins -->

  <!--差速驱动插件-->
  <gazebo>
    <plugin name="skid_steer_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <namespace>/shigu</namespace>
        <argument>/cmd_vel:cmd_vel</argument>
        <argument>/odom:=odom</argument>
      </ros>
      <update_rate>100.0</update_rate>
      <num_wheel_pairs>2</num_wheel_pairs>
 
      <left_joint>front_left_joint</left_joint>
      <left_joint>rear_left_joint</left_joint>
      <wheel_separation>0.65</wheel_separation>
      <wheel_diameter>0.4</wheel_diameter>
 
      <right_joint>front_right_joint</right_joint>
      <right_joint>rear_right_joint</right_joint>
      <wheel_separation>0.65</wheel_separation>
      <wheel_diameter>0.4</wheel_diameter>
 
      <max_wheel_torque>18</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>
 
      <publish_odom>true</publish_odom>
      <publish_odom_tf>false</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>

      <robot_base_frame>base_footprint</robot_base_frame>
      <odometry_frame>odom</odometry_frame>
      <!-- <odometry_source>world</odometry_source> -->
    </plugin>
  </gazebo>


  <!--six axis imu sensor-->
  <gazebo reference="imu_Link">
    <sensor name="my_imu" type="imu">
      <always_on>true</always_on>
      <!-- Publish at 30 hz -->
      <update_rate>30</update_rate>
      <plugin name="my_imu_plugin" filename="libgazebo_ros_imu_sensor.so">
        <ros>
          <!-- Will publish to /imu/data -->
          <namespace>/imu</namespace>
          <argument>~/out:=imu/data</argument>
        </ros>
        <!-- frame_name ommited, will be "imu_Link" -->
      </plugin>
    </sensor>
  </gazebo>
  
  <!--gps sensor is not allowed in ros2-->

  <!-- laser sensor -->
  <gazebo reference="laser_Link">
    <sensor type="ray" name="rplidar">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>5.5</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>360</samples>
            <resolution>1</resolution>
            <min_angle>-3.14</min_angle>
            <max_angle>3.14</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.4</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <update_rate>30</update_rate>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_ray_sensor.so">
        <ros>
          <namespace>/shigu</namespace>
          <argument>~/out:=scan</argument>
        </ros>
        <output_type>sensor_msgs/LaserScan</output_type>
      </plugin>
    </sensor>
  </gazebo>


  <!-- stereo camera -->
      <joint name="stereo_joint" type="fixed">
        <origin xyz="0 0 0" rpy="-1.5708 0 -1.5708"/>
        <parent link="camera_Link"/>
        <child link="stereo_link"/>
    </joint>
    <link name="stereo_link"/>

    <gazebo reference="camera_Link">  
      <sensor type="multicamera" name="stereocamera">
          <update_rate>60.0</update_rate>
          <always_on>true</always_on>
          <visualize>false</visualize>
          <camera name="left">
            <!-- <pose>0 0 0 0 0 0</pose> -->
            <horizontal_fov>1.3962634</horizontal_fov>
            <image>
              <width>1280</width>
              <height>720</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.02</near>
              <far>10</far>
            </clip>
            <distortion>
              <k1>0.0</k1>
              <k2>0.0</k2>
              <k3>0.0</k3>
              <p1>0.0</p1>
              <p2>0.0</p2>
              <center>0.0 0.0</center>
            </distortion>
          </camera>
          <camera name="right">
            <!-- <pose>0 0 0 0 0 0</pose> -->
            <horizontal_fov>1.3962634</horizontal_fov>
            <image>
              <width>1280</width>
              <height>720</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.02</near>
              <far>10</far>
            </clip>
            <distortion>
              <k1>0.0</k1>
              <k2>0.0</k2>
              <k3>0.0</k3>
              <p1>0.0</p1>
              <p2>0.0</p2>
              <center>-0.12 -0.12</center>
            </distortion>
          </camera>

          <plugin name="plugin_name" filename="libgazebo_ros_camera.so">
            <ros>
              <namespace>shigu</namespace>
              <!-- topics need to be prefixed with camera_name for remapping -->
              <argument>custom_camera/left/image_raw:=zed_node/left/image_raw</argument>
              <argument>custom_camera/right/image_raw:=zed_node/right/image_raw</argument>
              <argument>custom_camera/left/camera_info:=zed_node/left/camera_info</argument>
              <argument>custom_camera/right/camera_info:=zed_node/right/camera_info</argument>
            </ros>

            <camera_name>zed_node</camera_name>

            <frame_name>camera_Link</frameName>
          </plugin>
        </sensor>
    </gazebo>

</robot>